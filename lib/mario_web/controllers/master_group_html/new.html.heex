<h1>New Master Group</h1>

<.form for={@form} id="master-group-form" action={~p"/master_groups"}>
  <.input field={@form[:master_group_name]} label="Master Group Name" />
  <.input field={@form[:login_id]} label="Login ID" />
  <.input field={@form[:password]} label="Password" type="password" />

  <.button>Save</.button>
</.form>

<a href={~p"/master_groups"}>Back</a>
